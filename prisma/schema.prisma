// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
model Group {
  id        Int   @id
  nome      String   @unique
  membros    Aluno[]
  resumo    String
  nota      Float
  avaliador Avaliador[]
  }
model Aluno {
  matricula Int  @id
  nome  String
  nota  Float
  grupoId  Int
  grupo Group @relation(fields: [grupoId], references:[id])
}
model Avaliador {
  id       Int  @id 
  nome      String
  grupos    Group[]
  notas     Float
}